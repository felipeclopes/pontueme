<div class="wrapper">
	
	<div class="container">
		
		<div class="span4 offset4">
			
			<!-- start: Contact Form -->
			<div id="contact-form">
   
			    <%= devise_error_messages! %>

  				<div class="title"><h1>Registrar</h1></div>

		  			<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
		
					<p>Registre-se gratuitamente:</p>
        
			        <div class="field">
			          <label for="email">Email:</label>
			          <%= f.email_field :email, :class => 'login username-field' %>
			        </div> <!-- /field -->
			
					<div class="field">
			          <label for="senha">Senha:</label>
			          <%= f.password_field :password, :class => 'login password-field' %>
			        </div> <!-- /password -->
        
			      </div> <!-- /login-fields -->
    
  				<div class="login-actions">

			        <%= f.submit "Registrar", :class => "button btn btn-large" %>

			      </div> <!-- .actions -->

			    <% end %>

			  </div> <!-- /contact-form -->

		</div> <!-- /span4 -->

	</div> <!-- /container -->

</div> <!-- /wrapper -->